<evg-section
	class="evg-theme-earth-light"
	padding="fluid"
>
	<evg-wrapper size="xxl">
		<h2 class="evg-text-size-heading-md evg-spacing-bottom-lg evg-text-transform-uppercase">
			Circular LIVING STANDARD
		</h2>
		<evg-enter delay="0.5"type="fade-in-up">
			<evg-grid wrap="wrap">
				<evg-grid-item
					fill
					large-tablet="3"
					small-mobile="12"
					small-tablet="6"
				>
					<a href="https://wrap.ngo" class="ae-standards-list">
						<evg-card
							class="evg-theme-default"
							radius="md"
							layout="image-bottom"
						>
							<evg-card-content>
								<evg-grid justify-content="space-between">
									<evg-grid-item grow shrink>
										<h3 class="evg-text-size-body-md evg-text-family-body evg-text-transform-uppercase">
											PRELOVED
										</h3>
									</evg-grid-item>
									<evg-grid-item>
										<svg xmlns="http://www.w3.org/2000/svg" width="22" height="23" viewBox="0 0 22 23" fill="none">
										    <path d="M19.9999 1.77832L2.52954 20.3228" stroke="#0B301A" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"/>
										    <path d="M19.9993 20.8837V1.77832H2" stroke="#0B301A" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"/>
										</svg>
									</evg-grid-item>
								</evg-grid>
							</evg-card-content>
							<evg-card-img>
								<evg-img
								  aspect-ratio="auto"
								  block
								  object-fit="contain"
								  object-position="50% 50%"
								  responsive
								>
									<img
										alt="Placeholder"
										height="300"
										loading="lazy"
										src="media/images/preloved_certified.svg"
										width="400"
									/>
							</evg-img>
							</evg-card-img>
							
						</evg-card>
					</a>
				</evg-grid-item>

